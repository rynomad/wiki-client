{
    "name": "Wiki",
    "description": "Wiki - For dealing with '.wik's",
    "version": "1.0.0",
    "icons":{
    },
    "manifest_version": 2,
    "permissions":[
      "file://**/*.wik",
      "http://*/",
      "tabs",
      "activeTab"
    ],
    "web_accessible_resources":[
      "images/*.png",
      "*.png",
      "*.css",
      "js/*.css",
      "style/*.css",
      "js/dist/*.*",
      "js/images/*.png",
      "chrome/*.js",
      "*.map",
      "*.max.*",
      "plugins/wiki-plugin-*/client/*.js",
      "node_modules/wiki-client/client/*.*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"app": {
	  "launch": {
	    "local_path": "index.html"
	  }
	},
	"content_scripts":[
	  {
        "matches": ["http://*/*"],
        "js":[
          "chrome/checkWiki.js"
        ]
      }
    ],
    "background": {
      "scripts":["chrome/bootStrap.js"]
    },
    "offline_enabled": true
}
